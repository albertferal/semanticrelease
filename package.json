{
    "name": "liberando-productos-practica-final",
    "repository": {
      "type": "git",
      "url": "git@github.com:KeepCodingCloudDevops8/liberando-productos-practica-final.git"
    },
    "dependencies": {
      "@semantic-release/exec": "6.0.3",
      "@semantic-release/git": "10.0.1",
      "@semantic-release/github": "9.2.6",
      "@semantic-release/release-notes-generator": "12.1.0",
      "@semantic-release/changelog": "6.0.3",
      "semantic-release": "^22.0.12"
    },
    "release": {
      "branches": [
        "main"
      ],
      "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/github",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "docs/CHANGELOG.md"
            }
        ],
        [
          "@semantic-release/exec",
          {
            "verifyReleaseCmd": "./ci-utils/update_versions.sh ${nextRelease.version}",
            "prepareCmd": "echo ${nextRelease.version} > semantic_release_version.txt"
          }
        ],
        [
          "@semantic-release/git",
          {
            "assets": [
              "helm/**/*"
            ],
            "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
          }
        ]
      ]
    },
    "version": "0.0.1",
    "devDependencies": {
      "minimist": "^1.2.5"
    }
  }